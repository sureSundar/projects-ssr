<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @post.title %>
</p>

<p>
  <b>Content:</b>
  <%= @post.content %>
</p>

<p>
  <b>Comments:</b>
  <% for commentObj in @post.comments %>
  <p>
    <%= commentObj.comment %>
    </p>
  <% end %>
</p>
<%= link_to 'New Comment', new_post_comment_path(@post) %> 

